{
	"name": "wyreferee",
	"version": "6.7.1",
	"description": "Referee client",
	"author": {
		"name": "Wesley"
	},
	"keywords": [
		"osu",
		"irc",
		"axs",
		"bancho",
		"bancho.js",
		"referee client"
	],
	"main": "main.js",
	"private": true,
	"scripts": {
		"gh-publish-windows": "yarn run build:prod && electron-builder -p always",
		"gh-publish-linux": "yarn run build:prod && electron-builder build --linux -p always",
		"gh-publish-mac": "yarn run build:prod && electron-builder build --mac -p always",
		"gh-publish-mac-arm": "yarn run build:prod && electron-builder build --mac --arm64 -p always",
		"postinstall": "yarn run postinstall:electron && electron-builder install-app-deps",
		"postinstall:web": "node postinstall-web",
		"postinstall:electron": "node postinstall",
		"ng": "ng",
		"start": "yarn run postinstall:electron && npm-run-all -p ng:serve electron:serve",
		"build": "yarn run postinstall:electron && yarn run electron:serve-tsc && ng build",
		"build:dev": "yarn run build -- -c dev",
		"build:prod": "yarn run build -- -c production",
		"ng:serve": "ng serve",
		"ng:serve:web": "yarn run postinstall:web && ng serve -o",
		"electron:serve-tsc": "tsc -p tsconfig.serve.json",
		"electron:serve": "wait-on http-get://localhost:4200/ && yarn run electron:serve-tsc && electron . --serve",
		"electron:local": "yarn run build:prod && electron .",
		"electron:linux": "yarn run build:prod && electron-builder build --linux --publish never",
		"electron:windows": "yarn run build:prod && electron-builder build --windows --publish never",
		"electron:mac": "yarn run build:prod && electron-builder build --mac --publish never",
		"test": "yarn run postinstall:web && ng test",
		"e2e": "yarn run build:prod && playwright test -c e2e/playwright.config.ts e2e/",
		"lint": "ng lint"
	},
	"devDependencies": {
		"@angular-builders/custom-webpack": "13.1.0",
		"@angular-devkit/build-angular": "13.3.2",
		"@angular-eslint/builder": "13.1.0",
		"@angular-eslint/eslint-plugin": "13.1.0",
		"@angular-eslint/eslint-plugin-template": "13.1.0",
		"@angular-eslint/schematics": "13.1.0",
		"@angular-eslint/template-parser": "13.1.0",
		"@angular/cdk": "13.3.2",
		"@angular/cli": "13.3.2",
		"@angular/common": "13.3.2",
		"@angular/compiler": "13.3.2",
		"@angular/compiler-cli": "13.3.2",
		"@angular/core": "13.3.2",
		"@angular/forms": "13.3.2",
		"@angular/language-service": "13.3.2",
		"@angular/platform-browser": "13.3.2",
		"@angular/platform-browser-dynamic": "13.3.2",
		"@angular/router": "13.3.2",
		"@playwright/test": "^1.20.2",
		"@types/howler": "^2.1.2",
		"@types/jasmine": "3.3.16",
		"@types/jasminewd2": "2.0.6",
		"@types/node": "^15.12.5",
		"@typescript-eslint/eslint-plugin": "5.17.0",
		"@typescript-eslint/parser": "5.17.0",
		"codelyzer": "^6.0.0",
		"core-js": "3.6.5",
		"electron": "^13.1.4",
		"electron-builder": "23.0.3",
		"electron-reload": "1.5.0",
		"eslint": "^7.8.1",
		"eslint-plugin-import": "^2.22.0",
		"eslint-plugin-jsdoc": "^43.1.1",
		"eslint-plugin-prefer-arrow": "1.2.3",
		"howler": "^2.1.3",
		"jasmine-core": "^4.0.1",
		"jasmine-spec-reporter": "^7.0.0",
		"karma": "^6.3.17",
		"karma-chrome-launcher": "^3.1.1",
		"karma-coverage-istanbul-reporter": "^3.0.3",
		"karma-jasmine": "^4.0.2",
		"karma-jasmine-html-reporter": "^1.7.0",
		"npm-run-all": "4.1.5",
		"playwright": "^1.20.2",
		"rxjs": "7.5.5",
		"ts-node": "10.7.0",
		"tslib": "^2.1.0",
		"typescript": "~4.6.3",
		"wait-on": "6.0.1",
		"webdriver-manager": "12.1.8",
		"zone.js": "~0.11.4"
	},
	"engines": {
		"node": ">=14.0.0"
	},
	"dependencies": {
		"@angular/animations": "13.3.2",
		"@angular/material": "13.3.2",
		"bancho.js": "^0.9.7",
		"electron-log": "^3.0.8",
		"electron-store": "^5.0.0",
		"electron-updater": "^4.1.2",
		"ngx-clipboard": "^12.3.1",
		"ngx-markdown": "^13.1.0",
		"ngx-virtual-scroller": "^3.0.3",
		"semver": "^7.3.6"
	}
}
