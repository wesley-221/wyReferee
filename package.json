{
	"name": "wyreferee",
	"version": "7.0.0",
	"description": "Referee client",
	"author": {
		"name": "Wesley"
	},
	"keywords": [
		"osu",
		"irc",
		"axs",
		"bancho",
		"bancho.js",
		"referee client"
	],
	"main": "dist/electron/main.js",
	"private": true,
	"scripts": {
		"gh-publish-windows": "yarn run build:prod && electron-builder -p always",
		"gh-publish-linux": "yarn run build:prod && electron-builder build --linux -p always",
		"gh-publish-mac": "yarn run build:prod && electron-builder build --mac -p always",
		"gh-publish-mac-arm": "yarn run build:prod && electron-builder build --mac --arm64 -p always",
		"postinstall": "yarn run postinstall:electron && electron-builder install-app-deps",
		"postinstall:web": "node postinstall-web",
		"postinstall:electron": "node postinstall",
		"ng": "ng",
		"start": "yarn run postinstall:electron && npm-run-all -p ng:serve electron:serve",
		"build": "yarn run postinstall:electron && yarn run electron:serve-tsc && ng build",
		"build:dev": "yarn run build -c dev",
		"build:prod": "yarn run build -c production",
		"ng:serve": "ng serve",
		"ng:serve:web": "yarn run postinstall:web && ng serve -o",
		"electron:serve-tsc": "tsc -p tsconfig.electron.json",
		"electron:serve": "wait-on http-get://localhost:4200/ && yarn run electron:serve-tsc && electron . --serve",
		"electron:local": "yarn run build:prod && electron .",
		"electron:linux": "yarn run build:prod && electron-builder build --linux --publish never",
		"electron:windows": "yarn run build:prod && electron-builder build --windows --publish never",
		"electron:mac": "yarn run build:prod && electron-builder build --mac --publish never",
		"test": "yarn run postinstall:web && ng test",
		"lint": "ng lint"
	},
	"devDependencies": {
		"@angular-builders/custom-webpack": "18.0.0",
		"@angular-eslint/builder": "16.3.1",
		"@angular-eslint/eslint-plugin": "16.3.1",
		"@angular-eslint/eslint-plugin-template": "16.3.1",
		"@angular-eslint/schematics": "16.3.1",
		"@angular-eslint/template-parser": "16.3.1",
		"@angular/build": "^18.2.21",
		"@angular/cdk": "18.2.14",
		"@angular/cli": "18.2.21",
		"@angular/common": "18.2.14",
		"@angular/compiler": "18.2.14",
		"@angular/compiler-cli": "18.2.14",
		"@angular/core": "18.2.14",
		"@angular/forms": "18.2.14",
		"@angular/language-service": "18.2.14",
		"@angular/platform-browser": "18.2.14",
		"@angular/platform-browser-dynamic": "18.2.14",
		"@angular/router": "18.2.14",
		"@playwright/test": "^1.20.2",
		"@types/archiver": "^7.0.0",
		"@types/howler": "^2.1.2",
		"@types/jasmine": "3.3.16",
		"@types/node": "18.16.18",
		"@typescript-eslint/eslint-plugin": "5.17.0",
		"@typescript-eslint/parser": "5.17.0",
		"electron": "15.5.7",
		"electron-builder": "23.0.3",
		"electron-reload": "1.5.0",
		"eslint": "^7.8.1",
		"eslint-plugin-import": "^2.22.0",
		"howler": "^2.1.3",
		"jasmine-core": "^4.0.1",
		"jasmine-spec-reporter": "^7.0.0",
		"karma": "^6.3.17",
		"karma-chrome-launcher": "^3.1.1",
		"karma-jasmine": "^4.0.2",
		"karma-jasmine-html-reporter": "^1.7.0",
		"npm-run-all": "4.1.5",
		"rxjs": "7.5.5",
		"ts-node": "10.7.0",
		"tslib": "^2.1.0",
		"typescript": "~5.4.5",
		"wait-on": "6.0.1",
		"zone.js": "~0.14.10"
	},
	"engines": {
		"node": ">=14.0.0"
	},
	"dependencies": {
		"@angular/animations": "18.2.14",
		"@angular/material": "18.2.14",
		"archiver": "5.3.2",
		"bancho.js": "^0.12.0",
		"electron-log": "^3.0.8",
		"electron-updater": "6.6.2",
		"express": "^4.21.1",
		"ngx-markdown": "18.1.0",
		"marked": "^11.1.1",
		"semver": "^7.3.6"
	}
}
