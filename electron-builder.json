{
	"productName": "wyReferee",
	"directories": {
		"output": "release/"
	},
	"files": [
		"**/*",
		"!**/*.ts",
		"!*.code-workspace",
		"!LICENSE.md",
		"!package.json",
		"!package-lock.json",
		"!src/",
		"!e2e/",
		"!hooks/",
		"!angular.json",
		"!_config.yml",
		"!karma.conf.js",
		"!tsconfig.json"
	],
	"win": {
		"icon": "src/assets/images/icon.png",
		"target": [
			"nsis"
		]
	},
	"linux": {
		"icon": "src/assets/images/icon.png",
		"target": [
			"AppImage"
		]
	}
}
