{
	"productName": "wyReferee",
	"publish": [
		{
			"provider": "github",
			"owner": "wesley-221",
			"repo": "wyReferee"
		}
	],
	"directories": {
		"output": "release/"
	},
	"files": [
		"dist/**/*",
		"node_modules/**/*",
		"package.json"
	],
	"win": {
		"icon": "src/assets/images/icon.png",
		"target": [
			"nsis"
		]
	},
	"linux": {
		"icon": "src/assets/images/icon.png",
		"target": [
			"AppImage"
		]
	},
	"mac": {
		"icon": "src/assets/images/mac-icon/icon.png",
		"target": [
			"dmg",
			"zip"
		]
	}
}
